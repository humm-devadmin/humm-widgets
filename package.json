{
  "name": "humm-widgets",
  "version": "1.0.0",
  "description": "Interactive drop in banners for Humm",
  "main": "price-info.js",
  "dependencies": {
    "jquery": "^3.1.1",
    "remodal": "^1.1.0",
    "sass": "^1.18.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.2.2",
    "typescript": "^2.0.10",
    "webpack": "^1.13.3"
  },
  "scripts": {
    "css-shared": "sass -s compressed css/humm-branding.scss:css/humm-branding.css",
    "css-au": "sass -s compressed src/au/styles/priceInfoModal.scss:src/au/styles/priceInfoModal.min.css src/au/styles/priceInfoV2.scss:src/au/styles/priceInfoV2.min.css src/au/styles/moreInfoModal.scss:src/au/styles/moreInfoModal.min.css src/au/html/landing-page/styles/landing.scss:src/au/html/landing-page/styles/landing.css",
    "css-nz": "sass -s compressed src/nz/styles/priceInfoModal.scss:src/nz/styles/priceInfoModal.min.css src/nz/styles/moreInfoModal.scss:src/nz/styles/moreInfoModal.min.css",
    "css": "npm run css-shared && npm run css-au && npm run css-nz",
    "build": "npm run css && webpack",
    "watch": "npm run css && webpack --watch"
  },
  "author": "Humm",
  "license": "ISC"
}
